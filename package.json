{
  "name": "react-photo",
  "description": "my photo blog with react",
  "license": "MIT",
  "author": "",
  "homepage": "https://github.com/shoota/react-playground#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shoota/react-playground.git"
  },
  "bugs": {
    "url": "https://github.com/shoota/react-playground/issues"
  },
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=production",
    "postbuild": "cp ./meta/* public/",
    "build-storybook": "build-storybook",
    "generate:days": "ts-node src/data/daysGenerator.ts",
    "generate:works": "ts-node src/data/worksGenerator.ts",
    "lint": "eslint 'src/**/*.ts{,x}' --cache",
    "start": "webpack-dev-server",
    "storybook": "start-storybook -p 6006",
    "test": "tsc -p ./tsconfig.json"
  },
  "dependencies": {
    "@ant-design/icons": "4.0.3",
    "antd": "4.0.4",
    "dayjs": "1.8.23",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hot-loader": "4.12.20",
    "react-image": "2.4.0",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "styled-components": "5.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.9.0",
    "@babel/plugin-proposal-optional-chaining": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "7.9.0",
    "@hot-loader/react-dom": "17.0.2",
    "@storybook/addon-actions": "8.3.6",
    "@storybook/addon-knobs": "8.0.1",
    "@storybook/addon-links": "8.3.6",
    "@storybook/addons": "7.6.17",
    "@storybook/cli": "8.3.6",
    "@storybook/react": "8.3.6",
    "@types/clean-webpack-plugin": "2.0.2",
    "@types/copy-webpack-plugin": "10.1.3",
    "@types/faker": "6.6.11",
    "@types/file-loader": "5.0.4",
    "@types/html-webpack-plugin": "3.2.2",
    "@types/jest": "29.5.14",
    "@types/mini-css-extract-plugin": "2.5.1",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/react-router": "5.1.4",
    "@types/react-router-dom": "5.1.3",
    "@types/react-test-renderer": "18.3.0",
    "@types/styled-components": "5.0.1",
    "@types/webpack": "5.28.5",
    "@types/webpack-dev-server": "4.7.2",
    "@typescript-eslint/eslint-plugin": "8.11.0",
    "@typescript-eslint/parser": "8.11.0",
    "awesome-typescript-loader": "5.2.1",
    "babel-core": "6.26.3",
    "babel-loader": "9.2.1",
    "babel-plugin-react-remove-properties": "0.3.0",
    "babel-runtime": "6.26.0",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "12.0.2",
    "css-loader": "7.1.2",
    "eslint": "9.13.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "faker": "6.6.6",
    "file-loader": "6.0.0",
    "html-loader": "5.1.0",
    "html-webpack-plugin": "5.6.3",
    "husky": "9.1.6",
    "jest": "29.7.0",
    "less": "4.2.0",
    "less-loader": "12.2.0",
    "lint-staged": "15.2.10",
    "mini-css-extract-plugin": "2.9.1",
    "prettier": "3.3.3",
    "react-test-renderer": "18.3.1",
    "style-loader": "4.0.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.6.3",
    "webpack": "5.95.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.1.0"
  },
  "jest": {
    "collectCoverage": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/test/**.test.ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testURL": "http://localhost"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "resolutions": {
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1"
  }
}
